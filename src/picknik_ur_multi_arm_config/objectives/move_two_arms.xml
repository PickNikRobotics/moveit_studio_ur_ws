<?xml version='1.0' encoding='UTF-8'?>
<root BTCPP_format="4" main_tree_to_execute="Move Two Arms">
  <!-- ////////// -->
  <BehaviorTree ID="Move Two Arms" _description="Simple example objective for dual arm motions. Two arms are moved to different waypoints." _favorite="true" _hardcoded="false">
    <Decorator ID="KeepRunningUntilFailure">
      <Control ID="Sequence">
        <Control ID="Sequence">
        <!-- Move first arm down-->
          <SubTree ID="Move to Named Pose" waypoint_name="Arms Down" joint_group_name="fst_manipulator" _collapsed="true"/>
          <!-- Move second and fourth arm down-->
          <SubTree ID="Move to Named Pose" waypoint_name="Arms Down" joint_group_name="dual_arm_manipulator" _collapsed="true"/>
          <!-- Move fist arm home -->
          <SubTree ID="Move to Named Pose" waypoint_name="Home" joint_group_name="fst_manipulator" _collapsed="true"/>
          <!-- Move second and fourth arm arm home -->
          <SubTree ID="Move to Named Pose" waypoint_name="Home" joint_group_name="dual_arm_manipulator" _collapsed="true"/>
        </Control>
        <Control ID="Sequence">
          <!-- Move both arms down -->
          <SubTree ID="Move to Named Pose" waypoint_name="Arms Down" joint_group_name="multi_arm_manipulator" _collapsed="true"/>
          <!-- Move both arms home -->
          <SubTree ID="Move to Named Pose" waypoint_name="Home" joint_group_name="multi_arm_manipulator" _collapsed="true"/>
        </Control>
      </Control>
    </Decorator>
  </BehaviorTree>
</root>
