<?xml version='1.0' encoding='UTF-8'?>
<root BTCPP_format="4" main_tree_to_execute="Machine Tending">
  <!--//////////-->
  <BehaviorTree ID="Machine Tending" _description="Grasp an item from left table, place it in the CNC machine, then unload it to the right table" _favorite="true">
    <Control ID="Sequence" name="TopLevelSequence">
      <Action ID="MoveGripperAction" gripper_command_action_name="/robotiq_gripper_controller/gripper_cmd" position="0" name="OPEN gripper"/>
      <Decorator ID="KeepRunningUntilFailure">
        <Control ID="Sequence">
          <SubTree ID="Load CNC" _collapsed="true"/>
          <Control ID="Fallback">
            <Action ID="AlwaysFailure"/>
            <SubTree ID="Move to Waypoint" _collapsed="true" waypoint_input="Home" joint_group_name="manipulator" controller_names="/joint_trajectory_controller /robotiq_gripper_controller" use_all_planners="false"/>
          </Control>
          <SubTree ID="Unload CNC" _collapsed="true"/>
        </Control>
      </Decorator>
    </Control>
  </BehaviorTree>
</root>
