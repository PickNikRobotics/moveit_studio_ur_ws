<?xml version='1.0' encoding='UTF-8'?>
<root BTCPP_format="4" main_tree_to_execute="24 Solution - Airfoil Coverage Path">
  <!--//////////-->
  <BehaviorTree ID="24 Solution - Airfoil Coverage Path" _description="Estimate the pose of an airfoil and define a coverage path" _favorite="true" _hardcoded="false">
    <Control ID="Sequence" name="TopLevelSequence">
      <Action ID="ClearSnapshot"/>
      <SubTree ID="Move to Waypoint" waypoint_name="Look at Airfoil" joint_group_name="manipulator" controller_names="/joint_trajectory_controller /robotiq_gripper_controller" planner_interface="moveit_default"/>
      <SubTree ID="Estimate Object Pose" object_model_file_path="~/user_ws/install/picknik_ur_mobile_config/share/picknik_ur_mobile_config/description/assets/airfoil.stl" guess_position="4.5;0.0;1.0" guess_orientation="0.0;0.0;0.7071;0.7071" icp_max_correspondence_distance="0.5" model_to_real_pose="{model_to_real_pose}" _collapsed="true"/>
      <Action ID="TransformPose" input_pose="{model_to_real_pose}" translation_xyz="0.0; 0.2; -0.25" quaternion_xyzw="0.7071068; 0; 0; 0.7071068 "/>
      <Action ID="VisualizePose" pose="{output_pose}" marker_size="0.150000"/>
      <Action ID="GenerateCoveragePath" stride_distance="0.2" width="1" height="0.8" bottom_right_corner="{output_pose}"/>
      <Action ID="VisualizePath" path="{pose_stamped_vector}"/>
    </Control>
  </BehaviorTree>
</root>
