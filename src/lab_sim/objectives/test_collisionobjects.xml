<root BTCPP_format="4" main_tree_to_execute="TestcollisionObjects">
  <!--//////////-->
  <BehaviorTree ID="TestcollisionObjects" _description="" _favorite="false">
    <Control ID="Sequence" name="TopLevelSequence">
      <Action
        ID="CreateStampedPose"
        stamped_pose="{object_pose}"
        orientation_xyzw="0;0;0;1"
        position_xyz="0;0;0.1"
        reference_frame="grasp_link"
      />
      <Action ID="CreateGraspableObject" pose="{object_pose}" />
      <Action ID="ModifyObjectInPlanningScene" object="{cuboid_object}" />
      <Action
        ID="InitializeMTCTask"
        controller_names="/joint_trajectory_controller"
        task_id="mtc_task"
      />
      <Action ID="SetupMTCCurrentState" />
      <Action ID="SetupMTCAttachObject" object="{cuboid_object}" />
      <Action ID="PlanMTCTask" />
      <Action ID="ExecuteMTCTask" />
    </Control>
  </BehaviorTree>
</root>
