<root BTCPP_format="4" main_tree_to_execute="Visualize">
  <!--//////////-->
  <BehaviorTree ID="Visualize" _description="" _favorite="false">
    <Control ID="Sequence" name="TopLevelSequence">
      <Action
        ID="CreateStampedPose"
        orientation_xyzw="0;0;0;1"
        position_xyz="0;0;4"
      />
      <Action ID="VisualizePose" />
      <Action ID="AddPoseStampedToVector" input="{stamped_pose}" />
      <Action
        ID="AdjustPoseWithIMarker"
        initial_poses="{pose_stamped_vector}"
        prompts="Move Imarker"
      />
    </Control>
  </BehaviorTree>
</root>
