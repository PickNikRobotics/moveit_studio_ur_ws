<?xml version='1.0' encoding='UTF-8'?>
<root BTCPP_format="4" main_tree_to_execute="Visual Servoing Demo">
  <!-- ////////// -->
  <BehaviorTree ID="Visual Servoing Demo" _description="" _favorite="false" _hardcoded="false">
    <Control ID="Sequence" name="TopLevelSequence">
      <Action ID="CreateStampedPose" reference_frame="grasp_link" position_xyz="-0.4;0.0;0.0" orientation_xyzw="0.0;0.0;0.0;1.0" stamped_pose="{stamped_pose}"/>
      <Action ID="TransformPoseFrame" input_pose="{stamped_pose}" target_frame_id="world" output_pose="{output_pose}"/>
      <Action ID="ServoPose" controller_name="streaming_controller" target_pose="{output_pose}" planning_group_name="manipulator" max_translational_vel="0.1" max_rotational_vel="0.1" publish_rate="200" max_joint_acceleration="20.0" dry_run="0"/>
    </Control>
  </BehaviorTree>
</root>
