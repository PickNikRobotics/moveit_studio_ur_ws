<!-- UR5e inspired from https://github.com/google-deepmind/mujoco_menagerie/tree/main/universal_robots_ur5e -->

<mujoco model="ur5e scene">
  <include file="ur5e.xml"/>

    <asset>
        <!-- Define textures and materials -->
        <texture name="wood" type="cube" file="assets/wood.png"/>
        <material name="wood" texture="wood" texrepeat="1 1"/>

        <!-- Define AprilTag textures -->
        <texture name="apriltag1" type="2d" file="assets/tag36_11_00000.png"/>
        <texture name="apriltag2" type="2d" file="assets/tag36_11_00001.png"/>

        <!-- Define block materials with AprilTags -->
        <material name="apriltag_material1" texture="apriltag1" texuniform="false" />
        <material name="apriltag_material2" texture="apriltag2" texuniform="false" />
    </asset>

  <statistic center="0.3 0 0.4" extent="0.8"/>

  <visual>
    <headlight diffuse="0.6 0.6 0.6" ambient="0.1 0.1 0.1" specular="0 0 0"/>
    <rgba haze="0.15 0.25 0.35 1"/>
    <global azimuth="120" elevation="-20"/>
  </visual>

  <asset>
    <texture type="skybox" builtin="gradient" rgb1="0.3 0.5 0.7" rgb2="0 0 0" width="512" height="3072"/>
    <texture type="2d" name="groundplane" builtin="checker" mark="edge" rgb1="0.2 0.3 0.4" rgb2="0.1 0.2 0.3"
      markrgb="0.8 0.8 0.8" width="300" height="300"/>
    <material name="groundplane" texture="groundplane" texuniform="true" texrepeat="5 5" reflectance="0.2"/>
  </asset>  
  <asset>
    <texture type="2d" name="desk_texture" file="assets/desk.png"/>
    <texture type="2d" name="shelf_texture" file="assets/shelf.png"/>
    <texture type="2d" name="microscope_texture" file="assets/microscope.png"/>
    <texture type="2d" name="stirrer_texture" file="assets/stirrer.png"/>
    <texture type="2d" name="test_tubes_stand_texture" file="assets/test_tubes_stand.png"/>
    <texture type="2d" name="bottle_texture" file="assets/bottle.png"/>

    <material name="desk" texture="desk_texture" specular="0.5" shininess="0.5"/>
    <material name="shelf" texture="shelf_texture" specular="0.5" shininess="0.5"/>
    <material name="microscope" texture="microscope_texture" specular="0.5" shininess="0.5"/>
    <material name="glass" rgba="0.659 0.800 0.843 0.3"/>
    <material name="stirrer" texture="stirrer_texture" specular="0.5" shininess="0.5"/>
    <material name="test_tubes_stand" texture="test_tubes_stand_texture" specular="0.5" shininess="0.5"/>
    <material name="bottle" texture="bottle_texture" specular="0.5" shininess="0.5"/>

    <mesh file="desk.obj"/>
    <mesh file="shelf.obj"/>
    <mesh file="shelf_glass.obj"/>
    <mesh file="microscope.obj"/>
    <mesh file="beaker.obj"/>
    <mesh file="stirrer.obj"/>
    <mesh file="test_tubes.obj"/>
    <mesh file="test_tubes_stand.obj"/>
    <mesh file="flask.obj"/>
    <mesh file="bottle.obj"/>
  </asset>



  <worldbody>
    <light pos="0 0 1.5" dir="0 0 -1" directional="true"/>
    <geom name="floor" pos="0 0 0" size="0 0 0.05" type="plane" material="groundplane"/>

    <body name="desk" pos="0 0 0" euler="0 0 0">
      <geom material="desk" mesh="desk" class="visual"/>
    </body>
    <body name="shelf" pos="-2.78335 0.27681 0" euler="0 0 0">
      <geom material="shelf" mesh="shelf" class="visual"/>
    </body>
      <body name="shelf_glass" pos="-2.78335 0.27681 0" euler="0 0 0">
      <geom material="glass" mesh="shelf_glass" class="visual"/>
    </body>
    <body name="microscope" pos="-1.168 -0.3233 0.98385" euler="0 0 0">
      <geom material="microscope" mesh="microscope" class="visual"/>
    </body>
    <body name="beaker-1" pos="-1.34591 -0.022235 1.40977" euler="0 0 0">
      <geom material="glass" mesh="beaker" class="visual"/>
    </body>
    <body name="beaker-2" pos="-1.1503 -0.022235 1.40977" euler="0 0 45">
      <geom material="glass" mesh="beaker" class="visual"/>
    </body>
    <body name="beaker-3" pos="-0.871046 -0.370397 0.986592" euler="0 0 20">
      <geom material="glass" mesh="beaker" class="visual"/>
    </body>
    <body name="stirrer" pos="0.305 -0.339 0.985827" euler="0 0 0">
      <geom material="stirrer" mesh="stirrer" class="visual"/>
    </body>
    <body name="test_tubes_stand" pos="-0.7 -0.25 0.984476" euler="0 0 -30">
      <geom material="test_tubes_stand" mesh="test_tubes_stand" class="visual"/>
    </body>
      <body name="test_tubes" pos="-0.7 -0.25 0.984476" euler="0 0 -30">
      <geom material="glass" mesh="test_tubes" class="visual"/>
    </body>
    <body name="flask_1" pos="-0.5 -0.41 0.984476" euler="0 0 -30">
      <geom material="glass" mesh="flask" class="visual"/>
    </body>
    <body name="flask_2" pos="-0.36 -0.49 0.984476" euler="0 0 -30">
      <geom material="glass" mesh="flask" class="visual"/>
    </body>
    <body name="flask_3" pos="-0.3 -0.4 0.984476" euler="0 0 -30">
      <geom material="glass" mesh="flask" class="visual"/>
    </body>
    <body name="flask_4" pos="0.251 -0.428 1.0242" euler="0 0 -30">
      <geom material="glass" mesh="flask" class="visual"/>
    </body>
    <body name="bottle_1" pos="0 -0.4 0.985807" euler="0 0 -20">
      <geom material="bottle" mesh="bottle" class="visual"/>
    </body>
    <body name="bottle_2" pos="-0.1 -0.45 0.985807" euler="0 0 10">
      <geom material="bottle" mesh="bottle" class="visual"/>
    </body>
    <body name="bottle_3" pos="0.05 -0.45 0.985807" euler="0 0 -30">
      <geom material="bottle" mesh="bottle" class="visual"/>
    </body>
  </worldbody>

  <keyframe>
    <!-- qpos -->
    <!-- linear_rail, shoulder_pan, shoulder_lift, elbow, wrist_1, wrist_2, wrist_3 -->
    <!-- right_knuckle, right_finger, right_inner_knuckle, right_finger_tip, left_knuckle, left_finger, left_inner_knuckle, left_finger_tip -->
    <!-- block1 [x, y, z, qw, qx, qy, qz] -->
    <!-- block2 [x, y, z, qw, qx, qy, qz] -->
    <!-- block3 [x, y, z, qw, qx, qy, qz] -->
    <!-- ctrl -->
    <!-- linear_rail, shoulder_pan, shoulder_lift, elbow, wrist_1, wrist_2, wrist_3, robotiq_85_left_knuckle_joint -->
    <key qpos="
      0 0 -1.5707 1.5707 -1.8 -1.5707 0
      0 0 0 0 0 0 0 0
    "
    ctrl="
      0.8 0.126 -1.51 1.79 -2.2 -1.51 0 0
    "
    />
  </keyframe>
</mujoco>
