<?xml version='1.0' encoding='UTF-8'?>
<root BTCPP_format="4" main_tree_to_execute="Grasp Panel">
  <!-- ////////// -->
  <BehaviorTree ID="Grasp Panel">
    <Decorator ID="RetryUntilSuccessful" num_attempts="-1">
      <Control ID="ReactiveSequence">
        <Action ID="Find Panel"/>
        <Control ID="Fallback">
          <!-- Try to pick up the panel, if suction was unsuccessful move to the home position and start again -->
          <Action ID="Grab Panel"/>
          <Action ID="Move Home"/>
        </Control>
      </Control>
    </Decorator>
  </BehaviorTree>
</root>
