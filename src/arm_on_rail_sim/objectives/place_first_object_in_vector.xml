<?xml version='1.0' encoding='UTF-8'?>
<root BTCPP_format="4" main_tree_to_execute="Place first object in vector">
  <!--//////////-->
  <BehaviorTree ID="Place first object in vector" _subtreeOnly="true" _description="">
    <Decorator ID="ForEachPoseStamped" vector_in="{pose_vector}" out="{grasp_pose}">
      <Control ID="Sequence">
        <Action ID="LoadObjectiveParameters" config_file_name="place_object_config.yaml"/>
        <Action ID="InitializeMTCTask" task_id="place_object" controller_names="/joint_trajectory_controller /robotiq_gripper_controller" task="{place_object_task}"/>
        <Action ID="SetupMTCCurrentState" task="{place_object_task}"/>
        <Action ID="SetupMTCPickObject" grasp_pose="{grasp_pose}" task="{place_object_task}" parameters="{parameters}"/>
        <Action ID="PlanMTCTask" solution="{place_object_solution}" task="{place_object_task}"/>
        <SubTree ID="Wait for Trajectory Approval if User Available" solution="{place_object_solution}"/>
        <Action ID="ExecuteMTCTask" solution="{place_object_solution}"/>
      </Control>
    </Decorator>
  </BehaviorTree>
</root>
