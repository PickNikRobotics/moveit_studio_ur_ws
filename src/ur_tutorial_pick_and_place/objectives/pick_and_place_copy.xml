<?xml version='1.0' encoding='UTF-8'?>
<root BTCPP_format="4" main_tree_to_execute="Pick and Place copy">
  <!--//////////-->
  <BehaviorTree ID="Pick and Place copy" _description="" _favorite="true" _hardcoded="false">
    <Decorator ID="KeepRunningUntilFailure">
      <Control ID="Sequence" name="TopLevelSequence">
        <Control ID="Sequence">
          <Action ID="RetrieveWaypoint" joint_group_name="manipulator" waypoint_joint_state="{target_joint_state}" waypoint_name="Inspect"/>
          <Action ID="InitializeMTCTask" controller_names="/joint_trajectory_controller /robotiq_gripper_controller" task_id="custom_move_to_joint_state"/>
          <Action ID="SetupMTCMoveToJointState" joint_state="{target_joint_state}"/>
          <Action ID="PlanMTCTask"/>
          <Action ID="ExecuteMTCTask"/>
        </Control>
        <Control ID="Fallback">
          <SubTree ID="Move to Waypoint" _collapsed="false" joint_group_name="manipulator" controller_names="/joint_trajectory_controller /robotiq_gripper_controller" use_all_planners="false" waypoint_name="Place"/>
          <SubTree ID="Request Teleoperation" _collapsed="false" enable_user_interaction="true" user_interaction_prompt="Please move to Pick" initial_teleop_mode="3"/>
        </Control>
      </Control>
    </Decorator>
  </BehaviorTree>
</root>
